{
  "name": "pip-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "upgrade:all": "ncu -i -u",
    "local:build-android": "eas build --profile preview",
    "build:dev": "eas build --profile development --platform android",
    "local:prebuild": "npx expo prebuild --skip-dependency-update react-native,react,expo",
    "start": "npx expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "ts:check": "tsc",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [
      "./jest.setup.ts"
    ]
  },
  "dependencies": {
    "@expo/html-elements": "latest",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/checkbox": "^0.5.16",
    "@react-native-firebase/app": "^18.7.3",
    "@react-native-firebase/firestore": "^18.7.3",
    "@react-native-firebase/storage": "^18.7.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@shopify/react-native-skia": "0.1.196",
    "@types/react-native-snap-carousel": "^3.8.5",
    "axios": "^1.4.0",
    "d3-shape": "^3.2.0",
    "deprecated-react-native-prop-types": "^5.0.0",
    "eas": "^0.1.0",
    "expo": "^49.0.23",
    "expo-av": "~13.4.1",
    "expo-build-properties": "~0.8.3",
    "expo-dev-client": "~2.4.13",
    "expo-document-picker": "~11.5.4",
    "expo-image-picker": "~14.3.2",
    "expo-linear-gradient": "~12.3.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "firebase": "^10.7.1",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "link": "^1.5.1",
    "lottie-react-native": "5.1.6",
    "nativewind": "^2.0.11",
    "normalize-css-color": "^1.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.10",
    "react-native-animatable": "^1.3.3",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-masked-text": "^1.13.0",
    "react-native-pager-view": "6.2.0",
    "react-native-progress": "^5.0.1",
    "react-native-reanimated": "~3.3.0",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-select-dropdown": "^3.4.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "13.9.0",
    "react-native-svg-charts": "^5.3.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-webview": "13.2.2",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.2.2",
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react-native": "^12.4.1",
    "@types/jest": "^29.5.11",
    "@types/react-test-renderer": "^18.0.7",
    "react-native": "0.72.10",
    "react-native-svg-transformer": "^1.1.0",
    "tailwindcss": "3.3.2"
  },
  "private": true
}
